{
	"name": "roblox-deeplink-parser",
	"version": "0.1.25",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/RoSeal-Extension/Roblox-DeepLink-Parser.git"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/DeepLinkParser.d.ts",
				"default": "./dist/DeepLinkParser.js"
			}
		},
		"./authedProtocol": {
			"import": {
				"types": "./dist/AuthedProtocolParser.d.ts",
				"default": "./dist/AuthedProtocolParser.js"
			}
		},
		"./package.json": "./package.json"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "latest",
		"@biomejs/biome": "latest"
	},
	"scripts": {
		"build": "bunx bunup src/DeepLinkParser.ts src/AuthedProtocolParser.ts --silent"
	}
}